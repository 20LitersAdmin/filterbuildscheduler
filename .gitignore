# Ignore bundler config.
/.bundle


# Ignore db backups
/db/*.dump

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
.env

/node_modules
/yarn-error.log

.byebug_history

spec/examples.txt

.env
.env.local
/public/assets/*
.DS_Store
app/.DS_Store
app/controllers/.DS_Store

public/capybara.html

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/secrets.yml
